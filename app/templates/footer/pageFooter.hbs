{{#if data}}
<aside class="lo-full padding-off">
  <ul class="page-footer-view list list-inline">
    {{#each data}}
    {{#if attributes.url}}
    <li><a href="{{attributes.url}}" class="btn btn-primary btn-line">{{attributes.name}}</a></li>
    {{else}}
    <li>{{attributes.name}}</li>
    {{/if}}
    {{/each}}
  </ul>
</aside>
{{/if}}
